@model KurumsalProje.Models.Iletisim
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<div class="form">
    <form class="form-validate form-horizontal" id="feedback_form" method="post" action="/Iletisim/Edit/@Model.IletisimId" enctype="multipart/form-data">
        <div class="form-group ">
            <label for="cname" class="control-label col-lg-2">Adres <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Adres" name="Adres" minlength="5" type="text" required value="@Model.Adres" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cemail" class="control-label col-lg-2">Telefon <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Tel" name="Tel" minlength="5" type="text" required value="@Model.Tel" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cname" class="control-label col-lg-2">Fax <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Fax" name="Fax" minlength="5" type="text" required value="@Model.Fax" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cemail" class="control-label col-lg-2">Whatsapp <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Whatsapp" name="Whatsapp" minlength="5" type="text" required value="@Model.Whatsapp" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cname" class="control-label col-lg-2">FaceBook <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="facebook" name="facebook" minlength="5" type="text" required value="@Model.Facebook" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cemail" class="control-label col-lg-2">Twitter <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Twitter" name="Twitter" minlength="5" type="text" required value="@Model.Twitter" />
            </div>
        </div>
        <div class="form-group ">
            <label for="cname" class="control-label col-lg-2">Instagram <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="Instagram" name="Instagram" minlength="5" type="text" required value="@Model.Instagram" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-primary" type="submit">Güncelle</button>
                <button class="btn btn-default" type="button"><a href="/Iletisim/Index">Geri Dön</a></button>
            </div>
        </div>
        </form>
</div>








